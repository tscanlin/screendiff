{
  "name": "picdiff",
  "version": "1.0.0",
  "description": "Image diffing tool, useful for screenshot diffing",
  "main": "index.js",
  "scripts": {
    "lint": "standard --fix",
    "pretest": "yarn lint",
    "picdiff:validate": "node ./cli.js validate",
    "picdiff:update": "node ./cli.js update",
    "picdiff:diff": "node ./cli.js diff",
    "picdiff:clean": "node ./cli.js clean",
    "test": "jest --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tscanlin/picdiff.git"
  },
  "keywords": [
    "screenshot",
    "diff",
    "diffing",
    "picdiff"
  ],
  "author": "Tim Scanlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tscanlin/picdiff/issues"
  },
  "homepage": "https://github.com/tscanlin/picdiff#readme",
  "dependencies": {
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.0",
    "resemblejs": "^2.2.6",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "globby": "^6.1.0",
    "jest": "^21.0.2",
    "standard": "^10.0.3"
  },
  "standard": {
    "env": {
      "jest": true
    },
    "globals": [
      "describe",
      "expect",
      "test"
    ]
  }
}
